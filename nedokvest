import random

print('–ü—Ä–∏–≤–µ—Ç. –í —ç—Ç–æ–º –∫–≤–µ—Å—Ç–µ —Ç–µ–±–µ –ø—Ä–µ–¥—Å—Ç–æ–∏—Ç –ø—Ä–æ–∂–∏—Ç—å 3 –¥–Ω—è –∑–∞ —Ç–∞—Ä–∞–∫–∞–Ω–∞.')
print('–ü—Ä–∞–≤–∏–ª–∞ –æ—á–µ–Ω—å –ø—Ä–æ—Å—Ç—ã. –¢—ã –¥–æ–ª–∂–µ–Ω –≤—ã–±–∏—Ä–∞—Ç—å –¥–µ–π—Å—Ç–≤–∏—è, –æ—Ç —Ç–≤–æ–µ–≥–æ –≤—ã–±–æ—Ä–∞ –∑–∞–≤–∏—Å–∏—Ç –∏—Å—Ö–æ–¥.')
print('–î–ª—è –≤—ã–±–æ—Ä–∞ –¥–µ–π—Å—Ç–≤–∏—è –≤–≤–æ–¥–∏ —Ü–∏—Ñ—Ä—É, —Å–æ–æ—Ç–≤–µ—Ç—Å–≤—É—é—â—É—é –∂–µ–ª–∞–µ–º–æ–º—É –æ—Ç–≤–µ—Ç—É.')
print('–°–º–æ–∂–µ—à—å –ø—Ä–æ–∂–∏—Ç—å 3 –¥–Ω—è - —Ç—ã –≤—ã–∏–≥—Ä–∞–ª, —É–º—Ä–µ—à—å - –ø—Ä–æ–∏–≥—Ä–∞–ª.')
print('–£ —Ç–µ–±—è –µ—Å—Ç—å 3 —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∏: –∑–¥–æ—Ä–æ–≤—å–µ, —Å–æ–Ω–ª–∏–≤–æ—Å—Ç—å, —Å—ã—Ç–æ—Å—Ç—å. –í –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ç–≤–æ–µ–≥–æ –≤—ã–±–æ—Ä–∞, –æ–Ω–∏ —Ç–∞–∫–∂–µ –±—É–¥—É—Ç –º–µ–Ω—è—Ç—å—Å—è.')
print('–õ—É—á—à–µ –æ–±—Ä–∞—â–∞—Ç—å –Ω–∞ –Ω–∏—Ö –≤–Ω–∏–º–∞–Ω–∏–µ. –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫ - 50. –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —Ç–≤–æ–∏ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∏ —Ç–∞–∫–æ–≤—ã:')
health=50
sleep=0
food=50
print(f'–ó–¥–æ—Ä–æ–≤—å–µ: {health}')
print(f'–°–æ–Ω–ª–∏–≤–æ—Å—Ç—å: {sleep}')
print(f'–°—ã—Ç–æ—Å—Ç—å: {food}')
c=0
while c==0:
    print('===============')
    print('    –î–µ–Ω—å  1')
    print('===============')
    print('–¢—ã —Å–∞–º—ã–π –æ–±—ã—á–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω, –∂–∏–≤—É—â–∏–π –≤ –æ–±—â–µ–∂–∏—Ç–∏–∏. –ö–æ–º–Ω–∞—Ç–∞ 221 - —Ç–≤–æ–π –¥–æ–º.')
    print('–ö–æ–Ω–µ—á–Ω–æ —Ç–µ–±–µ —Ç—É—Ç –Ω–µ —Ä–∞–¥—ã –∏ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ –ø—ã—Ç–∞—é—Ç—Å—è —É–±–∏—Ç—å.')
    print('–ù–æ —Ç—ã –≥–∞–¥ –∂–∏–≤—É—á–∏–π.')
    print('===============')
    print('–û—á–µ—Ä–µ–¥–Ω–æ–π –æ—Å–µ–Ω–Ω–∏–π –¥–µ–Ω—å, —è—Ä–∫–æ —Å–≤–µ—Ç–∏—Ç —Å–æ–Ω—Ü–µ.')
    print('–¢—ã –ø—Ä–æ—Å—ã–ø–∞–µ—à—å—Å—è –≤ —Å–≤–æ–µ–º –º–∞–ª–µ–Ω—å–∫–æ–º —É–±–µ–∂–∏—â–µ.')
    print('–•–æ—á–µ—à—å –æ—Å–º–æ—Ç—Ä–µ—Ç—å—Å—è?')
    print('1. –î–∞')
    print('2. –ù–µ—Ç')
    otvet1=int(input())
    if otvet1==1:
        print('–¢—ã –≤—ã–ª–µ–∑–∞–µ—à—å –∏ –Ω–∞—á–∏–Ω–∞–µ—à—å –æ—Å–º–∞—Ç—Ä–∏–≤–∞—Ç—å—Å—è.')
        print('–í –∫–æ–º–Ω–∞—Ç–µ –Ω–∏–∫–æ–≥–æ –Ω–µ—Ç. –≠—Ç–æ –Ω–µ —É–¥–∏–≤–∏—Ç–µ–ª—å–Ω–æ, –≤–µ–¥—å —Å–µ–≥–æ–¥–Ω—è –≤—Ç–æ—Ä–Ω–∏–∫, –∞ –∑–Ω–∞—á–∏—Ç, –≤—Å–µ —É–µ—Ö–∞–ª–∏ –Ω–∞ —É—á–µ–±—É.')
        print('–ù–∞ —Å—Ç–æ–ª–µ –ª–µ–∂–∞—Ç –ø–∞—Ä—É –∫—Ä–æ—à–µ–∫, –∫–æ—Ç–æ—Ä—ã–µ –æ—Å—Ç–∞–≤–∏–ª–∏ –∑–∞ —Å–æ–±–æ–π –¥–æ–±—Ä–æ–¥—É—à–Ω—ã–µ —Å–æ—Å–µ–¥–∫–∏.')
        print('–ö—Ä–æ–º–µ —Å–µ–±—è, —Ç–∞—Ä–∞–∫–∞–Ω–æ–≤ –≤ –∫–æ–º–Ω–∞—Ç–µ —Ç—ã –Ω–µ –Ω–∞–±–ª—é–¥–∞–µ—à—å. –ü–æ—Ö–æ–∂–µ, —á—Ç–æ —Ç—ã - –µ–¥–∏–Ω—Å—Ç–≤–µ–Ω–Ω—ã–π –≤—ã–∂–∏–≤—à–∏–π.')
    elif otvet1==2:
        print(' ')
    else:
        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â–µ —Ä–∞–∑.')
    print('–ß–µ–º –∑–∞–π–º–µ—à—å—Å—è?')
    print('1. –ü–æ–∫—É—à–∞—Ç—å')
    print('2. –ü–æ—Å–ø–∞—Ç—å')
    print('3. –ü—Ä–æ–≥—É–ª—è—Ç—å—Å—è.')
    otvet2=int(input())
    if otvet2==1:
        print('–¢—ã –ø–æ–¥–Ω—è–ª—Å—è –Ω–∞ —Å—Ç–æ–ª –∫ –∑–∞–≤–µ—Ç–Ω—ã–º –∫—Ä–æ—à–∫–∞–º. –ó–∞–≤—Ç—Ä–∞–∫ –≤—ã–¥–∞–ª—Å—è –≤–∫—É—Å–Ω—ã–º –∏ –ø–∏—Ç–∞—Ç–µ–ª—å–Ω—ã–º.')
        food=50
        sleep+=10
    elif otvet2==2:
        print('–¢—ã —Ä–µ—à–∏–ª –ª–µ—á—å –ø–æ—Å–ø–∞—Ç—å. –ü—Ä–æ—Å–ø–∞–≤ –¥–æ –≤–µ—á–µ—Ä–∞, —Ç—ã –ø—Ä–æ—Å–Ω—É–ª—Å—è. –ñ–∏—Ç–µ–ª—å–Ω–∏—Ü—ã –∫–æ–º–Ω–∞—Ç—ã —É–∂–µ –≤–µ—Ä–Ω—É–ª–∏—Å—å.')
        print('–ù–∞—Ö–æ–¥–∏—Ç—å—Å—è –≤ –∫–æ–º–Ω–∞—Ç–µ —Å—Ç–∞–ª–æ –Ω–µ–º–Ω–æ–≥–æ –æ–ø–∞—Å–Ω–æ.')
        food-=10
        sleep=0
    elif otvet2==3:
        print('–¢—ã —Ä–µ—à–∏–ª –ø—Ä–æ–≥—É–ª—è—Ç—å—Å—è. –ö—É–¥–∞ –ø–æ–π–¥–µ–º?')
        print('1. –ù–∞ –∫—É—Ö–Ω—é')
        print('2. –í –∫–æ—Ä–∏–¥–æ—Ä')
        otv1=int(input())
        if otv1==1:
            variant=random.randint(1,3)
            if variant==1:
                print('–£ –ø–ª–∏—Ç—ã —Å—Ç–æ—è–ª –º–∞–ª—å—á–∏–∫. –û–Ω —á–µ–º-—Ç–æ —É–≤–ª–µ—á–µ–Ω–Ω–æ –∑–∞–Ω–∏–º–∞–ª—Å—è.')
                print('–û–∫–∞–∑–∞–ª–æ—Å—å, –æ–Ω –ø–µ–∫ –ø–∏—Ä–æ–≥–∏. –û–¥–Ω–∞ –ø–∞—Ä—Ç–∏—è —É–∂–µ —Å—Ç–æ—è–ª–∞ –Ω–∞ —Å—Ç–æ–ª–µ –∏ –≤—ã–≥–ª—è–¥–µ–ª–∞ –æ—á–µ–Ω—å –∞–ø–ø–µ—Ç–∏—Ç–Ω–æ.')
                print('–ü–æ–ø—ã—Ç–∞—Ç—å—Å—è –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å?')
                print('1. –î–∞')
                print('2. –ù–µ—Ç')
                o1=int(input())
                if o1==1:
                    print('–¢—ã —Ä–µ—à–∏–ª –æ—Ç–≤–µ–¥–∞—Ç—å —Å–ª–∞–¥–∫–æ–≥–æ –≤–∫—É—Å–Ω–æ–≥–æ –ø–∏—Ä–æ–≥–∞.')
                    a=random.randint(1,10)
                    if a<=5:
                        print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã –æ—Å—Ç–∞–ª—Å—è –Ω–µ–∑–∞–º–µ—á–µ–Ω—ã–º. –¢—ã –ø–ª–æ—Ç–Ω–æ –ø–æ–æ–±–µ–¥–∞–ª –∏ –æ—Å—Ç–∞–ª—Å—è –¥–æ–≤–æ–ª–µ–Ω.')
                        food=50
                        sleep+=10
                    if a>5:
                        print('–û –Ω–µ—Ç, —Ç–µ–±—è –∑–∞–º–µ—Ç–∏–ª–∏ –∏ –ø–æ–ø—ã—Ç–∞–ª–∏—Å—å –ø—Ä–∏—Ö–ª–æ–ø–Ω—É—Ç—å.')
                        a1=random.randint(1,10)
                        if a1<=7:
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å, –Ω–æ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω. –°—Ç–æ–∏—Ç –±—ã—Ç—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ!')
                            print('–£–≤—ã, —Ç—ã –Ω–µ —É—Å–ø–µ–ª —É—Ö–≤–∞—Ç–∏—Ç—å –¥–∞–∂–µ –∫—Ä–æ—à–∫—É.')
                            food-=10
                            sleep+=15
                            health-=20
                        if a1>7:
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª')
                            c+=1
                elif o1==2:
                    print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –ø–µ—Ä–µ–±–æ—Ä–æ–ª —Å–≤–æ—é —Ç—è–≥—É –∫–æ –≤–∫—É—Å–Ω—ã–º –ø–∏—Ä–æ–∂–∫–∞–º.')
                    print('–î–æ–≥–æ—Ç–æ–≤–∏–≤, –º–∞–ª—å—á–∏–∫ —É—à–µ–ª. –£–≤—ã, –∑–∞ —Å–æ–±–æ–π –æ–Ω –Ω–µ –æ—Å—Ç–∞–≤–∏–ª –Ω–∏ –æ–¥–Ω–æ–π –∫—Ä–æ—à–∫–∏. –¢–µ–±–µ –Ω–µ –ø–æ–≤–µ–∑–ª–æ.')
                    food-=10
                    sleep+=10
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            elif variant==2:
                print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–∏–∫–æ–≥–æ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å. –í —Ä–∞–∫–æ–≤–∏–Ω–µ —Ç—ã —É–≤–∏–¥–µ–ª –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ –∫–∞–ø–ª–∏ –≤–æ–¥—ã.')
                print('–•–æ—á–µ—à—å –ø–æ–ø–∏—Ç—å?')
                print('1. –î–∞')
                print('2. –ù–µ—Ç')
                o2=int(input())
                if o2==1:
                    print('–¢—ã —Ä–µ—à–∏–ª –∏—Å–ø–∏—Ç—å –≤–æ–¥—ã, –∑–∞–ª–µ–∑ –≤ —Ä–∞–∫–æ–≤–∏–Ω—É –∏ –ø—Ä–∏–Ω—è–ª—Å—è –∑–∞ –¥–µ–ª–æ.')
                    b=random.randint(1,10)
                    if b<=5:
                        print('–¢—ã –Ω–∞–ø–∏–ª—Å—è –≤–¥–æ–≤–æ–ª—å. –¢–≤–æ–µ–º—É —Å—á–∞—Å—Ç—å—é –Ω–µ—Ç –ø—Ä–µ–¥–µ–ª–∞.')
                        food=50
                        sleep+=10
                    if b>5:
                        print('–ö–∞–∫ —Ç–æ–ª—å–∫–æ —Ç—ã –Ω–∞—á–∞–ª –ø–∏—Ç—å —Å—Ç–æ–ª—å –∂–µ–ª–∞–Ω–Ω—É—é –≤–æ–¥—É, –∫—Ç–æ-—Ç–æ –∑–∞—à–µ–ª –Ω–∞ –∫—É—Ö–Ω—é.')
                        print('–ù–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–ª—å–Ω–æ –≤–∫–ª—é—á–∏–ª—Å—è –∫—Ä–∞–Ω –∏ —Ç–µ–±—è –Ω–∞—á–∞–ª–æ —Å–Ω–æ—Å–∏—Ç—å –ø–æ—Ç–æ–∫–æ–º –≤–æ–¥—ã.')
                        b1=random.randint(1,10)
                        if b1<=7:
                            print('–ö —Å—á–∞—Å—Ç—å—é, —Ç–µ–±–µ —É–¥–∞–ª–æ—Å—å –≤—ã–∂–∏—Ç—å. –¢—ã –µ–ª–µ –≤—ã–±—Ä–∞–ª—Å—è –∏ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                            print('–¢—ã –æ—Ç–±–µ–∂–∞–ª –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ, –≥–¥–µ —Ç–µ–±—è —Ç–æ—á–Ω–æ –Ω–µ –Ω–∞–π–¥—É—Ç.')
                            food=50
                            sleep+=15
                            health-=20
                        if b1>7:
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c+=1
                elif o2==2:
                    print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –æ—Å—Ç–∞–ª—Å—è –Ω–∞–±–ª—é–¥–∞—Ç—å –∑–∞ –∫—É—Ö–Ω–µ–π –≤ —É–≥–æ–ª–∫–µ.')
                    print('–í—Ä–µ–º—è –æ—Ç –≤—Ä–µ–º–µ–Ω–∏ –Ω–∞ –∫—É—Ö–Ω—é –∑–∞—Ö–æ–¥–∏–ª–∏ –ª—é–¥–∏ –∏ —á—Ç–æ-—Ç–æ –¥–µ–ª–∞–ª–∏.')
                    print('–¢—ã –¥—É–º–∞–ª –æ —Ç–æ–º, –∫–∞–∫–æ–≤–æ –±—ã—Ç—å —á–µ–ª–æ–≤–µ–∫–æ–º.')
                    print('–õ—é–¥–∏ —Ç–∞–∫–∏–µ –±–æ–ª—å—à–∏–µ, –ø–æ—Å—Ç–æ—è–Ω–Ω–æ —á–µ–º-—Ç–æ –∑–∞–Ω—è—Ç—ã, –∫—É–¥–∞-—Ç–æ —Ö–æ–¥—è—Ç, —Ä–∞–¥—É—é—Ç—Å—è, –≥—Ä—É—Å—Ç—è—Ç.')
                    print('–ö–∞–∫–æ–≤–∞ –∏—Ö —Ü–µ–ª—å? –û —á–µ–º –æ–Ω–∏ –¥—É–º–∞—é—Ç? –í—Å–µ —ç—Ç–∏ –≤–æ–ø—Ä–æ—Å—ã –æ—á–µ–Ω—å –∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–ª–∏ —Ç–µ–±—è.')
                    print('–ù–æ –æ—Ç–≤–µ—Ç—ã –Ω–∞ –Ω–∏—Ö —Ç—ã –≤—Ä—è–¥ –ª–∏ –ø–æ–ª—É—á–∏—à—å.')
                    food-=10
                    sleep+=10
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            elif variant==3:
                print('–û –Ω–µ—Ç, –Ω–∞ –∫—É—Ö–Ω–µ —Ç—Ä–∞–≤—è—Ç —Ç–∞—Ä–∞–∫–∞–Ω–æ–≤. –ï—Å–ª–∏ —Ö–æ—á–µ—à—å –≤—ã–∂–∏—Ç—å, —Ç–æ —Ç–µ–±–µ –ª—É—á—à–µ —Ç—É–¥–∞ –Ω–µ —Ö–æ–¥–∏—Ç—å.')
                print('–í—Å–µ —Ä–∞–≤–Ω–æ –ø–æ–π—Ç–∏ –Ω–∞ –∫—É—Ö–Ω—é?')
                print('1. –î–∞')
                print('2. –ù–µ—Ç')
                o3=int(input())
                if o3==1:
                    b2=random.randint(1,10)
                    if b2<=4:
                        print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                        print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —É —Ç–µ–±—è –æ–∫–∞–∑–∞–ª—Å—è –∏–º–º—É–Ω–∏—Ç–µ—Ç –∫ —ç—Ç–æ–π –æ—Ç—Ä–∞–≤–µ.')
                        print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å –Ω–∏—á–µ–≥–æ —Å—ä–µ—Å—Ç–Ω–æ–≥–æ.')
                        print('–ù–æ —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —Ç–∞—Ä–∞–∫–∞–Ω–∏–π –≥–µ–Ω–æ—Ü–∏–¥. –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –≥–µ—Ä–æ–µ–º!')
                        print('–î–µ—Ä–∂–∏ –º–µ–¥–∞–ª—å –∑–∞ —Ö—Ä–∞–±—Ä–æ—Å—Ç—å.')
                        print('ü•á')
                        food-=10
                        sleep+=10
                    if b2>4:
                        print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                        print('–¢—ã –Ω–∞—á–∞–ª –∑–∞–¥—ã—Ö–∞—Ç—å—Å—è.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c+=1
                elif o3==2:
                    print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å –∏ –Ω–µ –ø–æ—à–µ–ª –≤ –∑–∞—Ä–∞–∂–µ–Ω–Ω—É—é –∑–æ–Ω—É.')
                    food-=10
                    sleep+=10
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
        elif otv1==2:
            b33=random.randint(1,2)
            if b33==1:
                print('–¢—ã –≤—ã—à–µ–ª –∏–∑ –∫–æ–º–Ω–∞—Ç—ã –∏ –ø–æ–ø–∞–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä.')
                print('–í–¥—Ä—É–≥, –Ω–∞—á–∞–ª—Å—è –Ω–∞–ø–ª—ã–≤ –ª—é–¥–µ–π.')
                print('–û–≥—Ä–æ–º–Ω–∞—è —Ç–æ–ø–ª–∞ –Ω–∞–¥–≤–∏–≥–∞–ª–∞—Å—å –Ω–∞ —Ç–µ–±—è.')
                print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                print('1. –ü–æ–ø—ã—Ç–∞—Ç—å—Å—è —É–±–µ–∂–∞—Ç—å')
                print('2. –û—Å—Ç–∞—Ç—å—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ')
                o4=int(input())
                if o4==1:
                    print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –ø—Ä–∏–¥–µ—Ç—Å—è –ø–æ—Å—Ç–∞—Ä–∞—Ç—å—Å—è –∏ –ø–æ–Ω–∞–¥–µ—è—Ç—å—Å—è –Ω–∞ —Å–≤–æ—é —É–¥–∞—á—É.')
                    print('–¢–µ–±–µ –Ω—É–∂–Ω–æ –≤—ã–±—Ä–∞—Ç—å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤ –∫–æ—Ç–æ—Ä–æ–º –±–µ–∂–∞—Ç—å, —É–≥–∞–¥–∞–ª - –º–æ–ª–æ–¥–µ—Ü, –Ω–µ —É–≥–∞–¥–∞–ª - –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                    print('–ü–æ–µ—Ö–∞–ª–∏!')
                    print('========')
                    nn=0
                    while nn<=5:
                        b3=random.randint(1,2)
                        print('–ö—É–¥–∞?')
                        print('1. –í–ª–µ–≤–æ')
                        print('2. –í–ø—Ä–∞–≤–æ')
                        n=int(input())
                        if b3==1 and n==1:
                            print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                        elif b3==1 and n==2:
                            print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                            health-=5
                        elif b3==2 and n==2:
                            print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                        elif b3==2 and n==1:
                            print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                            health-=5
                        else:
                            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                        nn+=1
                        food-=2
                        sleep+=2
                    print('–£—Ä–∞! –¢—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —ç—Ç–æ–π –±–µ—Å–ø–æ—â–∞–¥–Ω–æ–π –ª—é–¥—Å–∫–æ–π —Ç–æ–ª–ø–µ.')
                elif o4==2:
                    print('–¢—ã –æ—Å—Ç–∞–ª—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ –∏ –ø—Ä–æ—Å—Ç–æ —Å–º–æ—Ç—Ä–µ–ª –∫–∞–∫ —Ç–æ–ª–ø–∞ —Å—Ç–∞–Ω–æ–≤–∏–ª–∞—Å—å –≤—Å–µ –±–ª–∏–∂–µ –∏ –±–ª–∏–∂–µ –∫ —Ç–µ–±–µ.')
                    b4=random.randint(1,10)
                    if b4<=5:
                        print('–ù–∞ —É–¥–∏–≤–ª–µ–Ω–∏–µ, –Ω–∞ —Ç–µ–±—è –Ω–∏–∫—Ç–æ –¥–∞–∂–µ –Ω–µ –Ω–∞—Å—Ç—É–ø–∏–ª.')
                        print('–¢—ã –æ—á–µ–Ω—å –≤–µ–∑—É—á–∏–π —Ç–∞—Ä–∞–∫–∞–Ω!')
                        sleep+=5
                        food-=5
                    if b4>5:
                        print('–ü–æ —Ç–µ–±–µ –ø—Ä–æ—à–ª–æ—Å—å —Ü–µ–ª—ã—Ö —Ç—Ä–∏ —á–µ–ª–æ–≤–µ–∫–∞.')
                        print('–¢–≤–æ–µ –º–∞–ª–µ–Ω—å–∫–æ–µ —Ç–∞—Ä–∞–∫–∞–Ω—å–µ —Ç–µ–ª—å—Ü–µ –Ω–µ —Å–º–æ–≥–ª–æ –≤—ã–¥–µ—Ä–∂–∞—Ç—å —Ç–∞–∫–æ–≥–æ –Ω–∞–ø–æ—Ä–∞.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c+=1
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            if b33==2:
                print('–¢—ã –≤—ã—à–µ–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä. –°–∑–∞–¥–∏ –ø–æ—Å–ª—ã—à–∞–ª—Å—è –∫–∞–∫–æ–π-—Ç–æ —Ç–æ–ø–æ—Ç.')
                print('–û–±–µ—Ä–Ω—É–≤—à–∏—Å—å, —Ç—ã —É–≤–∏–¥–µ–ª –≤—ã—Å–æ–∫–æ–≥–æ –º–∞–ª—å—á–∏–∫–∞ –∫–æ—Ç–æ—Ä—ã–π –±–µ–∂–∞–ª –Ω–∞ —Ç–µ–±—è.')
                print('–≠—Ç–æ –æ–∫–∞–∑–∞–ª—Å—è –°–∞—à–∞ –∏–∑ 205 –∫–æ–º–Ω–∞—Ç—ã. –ï–≥–æ –ª–∏—Ü–æ –≤—ã—Ä–∞–∂–∞–ª–æ —Å–∏–ª—å–Ω–æ–µ –∂–µ–ª–∞–Ω–∏–µ —Ä–∞—Å—Ç–æ–ø—Ç–∞—Ç—å —Ç–µ–±—è.')
                print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                print('1. –ë–µ–∂–∞—Ç—å')
                print('2. –û—Å—Ç–∞—Ç—å—Å—è –Ω–∞ –º–µ—Å—Ç–µ')
                o5=int(input())
                if o5==1:
                    print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –≤–æ–∑–≤–æ–¥–∏ —á–∏—Å–ª–∞ –ø–æ–∫–∞–∑–∞–Ω–Ω—ã–µ –Ω–∞ —ç–∫—Ä–∞–Ω–µ –≤ –∫–≤–∞–¥—Ä–∞—Ç.')
                    igra=0
                    while igra<=3:
                        chislo=random.randint(1,10)
                        print(chislo)
                        kvadrat=int(input())
                        if kvadrat==chislo**2:
                            print('–ü—Ä–∞–≤–∏–ª—å–Ω–æ. –ü—Ä–æ–¥–æ–ª–∂–∞–π –±–µ–∂–∞—Ç—å.')
                        else:
                            print('–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ. –°–∞—à–∞ –ø–æ—á—Ç–∏ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è, –Ω–∞—Å—Ç—É–ø–∏–≤ —Ç–µ–±–µ –Ω–∞ –ª–∞–ø–∫—É. –ë—É–¥—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ.')
                            print('–ü–æ–ª—É—á–µ–Ω —É—Ä–æ–Ω.')
                            health-=10
                        igra+=1
                    chislo=random.randint(1,10)
                    print(chislo)
                    kvadrat=int(input())
                    if kvadrat==chislo**2:
                        print('–£—Ä–∞, —Ç—ã —Å–º–æ–≥ —É–±–µ–∂–∞—Ç—å! –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –Ω–∞—Å—Ç–æ—è—â–∏–º —Å–ø–æ—Ä—Ç—Å–º–µ–Ω–æ–º.')
                    else:
                        print('–°–∞—à–∞ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è –∏ –Ω–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–Ω–æ –Ω–∞—Å—Ç—É–ø–∏–ª –Ω–∞ —Ç–µ–±—è —Å–≤–æ–µ–π –æ–≥—Ä–æ–º–Ω–æ–π –Ω–æ–≥–æ–π.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c+=1
                elif o5==2:
                    print('–°–∞—à–∞ –ø–æ–¥–±–µ–∂–∞–ª –∫ —Ç–µ–±–µ –∏ —Ä–∞–∑–¥–∞–≤–∏–ª.')
                    print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                    print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                    c+=1
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
        else:
            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
        print('–¢—ã –≤–µ—Ä–Ω—É–ª—Å—è –≤ –∫–æ–º–Ω–∞—Ç—É.')
    else:
        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    print(f'–ó–¥–æ—Ä–æ–≤—å–µ: {health}')
    print(f'–°–æ–Ω–ª–∏–≤–æ—Å—Ç—å: {sleep}')
    print(f'–°—ã—Ç–æ—Å—Ç—å: {food}')
    print('–û—Ç–ª–∏—á–Ω–æ. –ß–µ–º –∑–∞–π–º–µ—à—å—Å—è –¥–∞–ª—å—à–µ?')
    if otvet2==1:
        print('1. –ü–æ—Å–ø–∞—Ç—å')
        print('2. –ü—Ä–æ–≥—É–ª—è—Ç—å—Å—è')
        otvet3=int(input())
        if otvet3==1:
            print('–¢—ã —Ä–µ—à–∏–ª –ª–µ—á—å –ø–æ—Å–ø–∞—Ç—å. –ü—Ä–æ—Å–ø–∞–≤ –¥–æ –≤–µ—á–µ—Ä–∞, —Ç—ã –ø—Ä–æ—Å–Ω—É–ª—Å—è. –ñ–∏—Ç–µ–ª—å–Ω–∏—Ü—ã –∫–æ–º–Ω–∞—Ç—ã —É–∂–µ –≤–µ—Ä–Ω—É–ª–∏—Å—å.')
            print('–ù–∞—Ö–æ–¥–∏—Ç—å—Å—è –≤ –∫–æ–º–Ω–∞—Ç–µ —Å—Ç–∞–ª–æ –Ω–µ–º–Ω–æ–≥–æ –æ–ø–∞—Å–Ω–æ.')
            food -= 10
            sleep = 0
        elif otvet3==2:
            print('–¢—ã —Ä–µ—à–∏–ª –ø—Ä–æ–≥—É–ª—è—Ç—å—Å—è. –ö—É–¥–∞ –ø–æ–π–¥–µ–º?')
            print('1. –ù–∞ –∫—É—Ö–Ω—é')
            print('2. –í –∫–æ—Ä–∏–¥–æ—Ä')
            otv1 = int(input())
            if otv1 == 1:
                variant = random.randint(1, 3)
                if variant == 1:
                    print('–£ –ø–ª–∏—Ç—ã —Å—Ç–æ—è–ª –º–∞–ª—å—á–∏–∫. –û–Ω —á–µ–º-—Ç–æ —É–≤–ª–µ—á–µ–Ω–Ω–æ –∑–∞–Ω–∏–º–∞–ª—Å—è.')
                    print('–û–∫–∞–∑–∞–ª–æ—Å—å, –æ–Ω –ø–µ–∫ –ø–∏—Ä–æ–≥–∏. –û–¥–Ω–∞ –ø–∞—Ä—Ç–∏—è —É–∂–µ —Å—Ç–æ—è–ª–∞ –Ω–∞ —Å—Ç–æ–ª–µ –∏ –≤—ã–≥–ª—è–¥–µ–ª–∞ –æ—á–µ–Ω—å –∞–ø–ø–µ—Ç–∏—Ç–Ω–æ.')
                    print('–ü–æ–ø—ã—Ç–∞—Ç—å—Å—è –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o1 = int(input())
                    if o1 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –æ—Ç–≤–µ–¥–∞—Ç—å —Å–ª–∞–¥–∫–æ–≥–æ –≤–∫—É—Å–Ω–æ–≥–æ –ø–∏—Ä–æ–≥–∞.')
                        a = random.randint(1, 10)
                        if a <= 5:
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã –æ—Å—Ç–∞–ª—Å—è –Ω–µ–∑–∞–º–µ—á–µ–Ω—ã–º. –¢—ã –ø–ª–æ—Ç–Ω–æ –ø–æ–æ–±–µ–¥–∞–ª –∏ –æ—Å—Ç–∞–ª—Å—è –¥–æ–≤–æ–ª–µ–Ω.')
                            food = 50
                            sleep += 10
                        if a > 5:
                            print('–û –Ω–µ—Ç, —Ç–µ–±—è –∑–∞–º–µ—Ç–∏–ª–∏ –∏ –ø–æ–ø—ã—Ç–∞–ª–∏—Å—å –ø—Ä–∏—Ö–ª–æ–ø–Ω—É—Ç—å.')
                            a1 = random.randint(1, 10)
                            if a1 <= 7:
                                print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å, –Ω–æ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω. –°—Ç–æ–∏—Ç –±—ã—Ç—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ!')
                                print('–£–≤—ã, —Ç—ã –Ω–µ —É—Å–ø–µ–ª —É—Ö–≤–∞—Ç–∏—Ç—å –¥–∞–∂–µ –∫—Ä–æ—à–∫—É.')
                                food -= 10
                                sleep += 15
                                health -= 20
                            if a1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª')
                                c += 1
                    elif o1 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –ø–µ—Ä–µ–±–æ—Ä–æ–ª —Å–≤–æ—é —Ç—è–≥—É –∫–æ –≤–∫—É—Å–Ω—ã–º –ø–∏—Ä–æ–∂–∫–∞–º.')
                        print('–î–æ–≥–æ—Ç–æ–≤–∏–≤, –º–∞–ª—å—á–∏–∫ —É—à–µ–ª. –£–≤—ã, –∑–∞ —Å–æ–±–æ–π –æ–Ω –Ω–µ –æ—Å—Ç–∞–≤–∏–ª –Ω–∏ –æ–¥–Ω–æ–π –∫—Ä–æ—à–∫–∏. –¢–µ–±–µ –Ω–µ –ø–æ–≤–µ–∑–ª–æ.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 2:
                    print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–∏–∫–æ–≥–æ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å. –í —Ä–∞–∫–æ–≤–∏–Ω–µ —Ç—ã —É–≤–∏–¥–µ–ª –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ –∫–∞–ø–ª–∏ –≤–æ–¥—ã.')
                    print('–•–æ—á–µ—à—å –ø–æ–ø–∏—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o2 = int(input())
                    if o2 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –∏—Å–ø–∏—Ç—å –≤–æ–¥—ã, –∑–∞–ª–µ–∑ –≤ —Ä–∞–∫–æ–≤–∏–Ω—É –∏ –ø—Ä–∏–Ω—è–ª—Å—è –∑–∞ –¥–µ–ª–æ.')
                        b = random.randint(1, 10)
                        if b <= 5:
                            print('–¢—ã –Ω–∞–ø–∏–ª—Å—è –≤–¥–æ–≤–æ–ª—å. –¢–≤–æ–µ–º—É —Å—á–∞—Å—Ç—å—é –Ω–µ—Ç –ø—Ä–µ–¥–µ–ª–∞.')
                            food = 50
                            sleep += 10
                        if b > 5:
                            print('–ö–∞–∫ —Ç–æ–ª—å–∫–æ —Ç—ã –Ω–∞—á–∞–ª –ø–∏—Ç—å —Å—Ç–æ–ª—å –∂–µ–ª–∞–Ω–Ω—É—é –≤–æ–¥—É, –∫—Ç–æ-—Ç–æ –∑–∞—à–µ–ª –Ω–∞ –∫—É—Ö–Ω—é.')
                            print('–ù–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–ª—å–Ω–æ –≤–∫–ª—é—á–∏–ª—Å—è –∫—Ä–∞–Ω –∏ —Ç–µ–±—è –Ω–∞—á–∞–ª–æ —Å–Ω–æ—Å–∏—Ç—å –ø–æ—Ç–æ–∫–æ–º –≤–æ–¥—ã.')
                            b1 = random.randint(1, 10)
                            if b1 <= 7:
                                print('–ö —Å—á–∞—Å—Ç—å—é, —Ç–µ–±–µ —É–¥–∞–ª–æ—Å—å –≤—ã–∂–∏—Ç—å. –¢—ã –µ–ª–µ –≤—ã–±—Ä–∞–ª—Å—è –∏ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                                print('–¢—ã –æ—Ç–±–µ–∂–∞–ª –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ, –≥–¥–µ —Ç–µ–±—è —Ç–æ—á–Ω–æ –Ω–µ –Ω–∞–π–¥—É—Ç.')
                                food = 50
                                sleep += 15
                                health -= 20
                            if b1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                                c += 1
                    elif o2 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –æ—Å—Ç–∞–ª—Å—è –Ω–∞–±–ª—é–¥–∞—Ç—å –∑–∞ –∫—É—Ö–Ω–µ–π –≤ —É–≥–æ–ª–∫–µ.')
                        print('–í—Ä–µ–º—è –æ—Ç –≤—Ä–µ–º–µ–Ω–∏ –Ω–∞ –∫—É—Ö–Ω—é –∑–∞—Ö–æ–¥–∏–ª–∏ –ª—é–¥–∏ –∏ —á—Ç–æ-—Ç–æ –¥–µ–ª–∞–ª–∏.')
                        print('–¢—ã –¥—É–º–∞–ª –æ —Ç–æ–º, –∫–∞–∫–æ–≤–æ –±—ã—Ç—å —á–µ–ª–æ–≤–µ–∫–æ–º.')
                        print('–õ—é–¥–∏ —Ç–∞–∫–∏–µ –±–æ–ª—å—à–∏–µ, –ø–æ—Å—Ç–æ—è–Ω–Ω–æ —á–µ–º-—Ç–æ –∑–∞–Ω—è—Ç—ã, –∫—É–¥–∞-—Ç–æ —Ö–æ–¥—è—Ç, —Ä–∞–¥—É—é—Ç—Å—è, –≥—Ä—É—Å—Ç—è—Ç.')
                        print('–ö–∞–∫–æ–≤–∞ –∏—Ö —Ü–µ–ª—å? –û —á–µ–º –æ–Ω–∏ –¥—É–º–∞—é—Ç? –í—Å–µ —ç—Ç–∏ –≤–æ–ø—Ä–æ—Å—ã –æ—á–µ–Ω—å –∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–ª–∏ —Ç–µ–±—è.')
                        print('–ù–æ –æ—Ç–≤–µ—Ç—ã –Ω–∞ –Ω–∏—Ö —Ç—ã –≤—Ä—è–¥ –ª–∏ –ø–æ–ª—É—á–∏—à—å.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 3:
                    print('–û –Ω–µ—Ç, –Ω–∞ –∫—É—Ö–Ω–µ —Ç—Ä–∞–≤—è—Ç —Ç–∞—Ä–∞–∫–∞–Ω–æ–≤. –ï—Å–ª–∏ —Ö–æ—á–µ—à—å –≤—ã–∂–∏—Ç—å, —Ç–æ —Ç–µ–±–µ –ª—É—á—à–µ —Ç—É–¥–∞ –Ω–µ —Ö–æ–¥–∏—Ç—å.')
                    print('–í—Å–µ —Ä–∞–≤–Ω–æ –ø–æ–π—Ç–∏ –Ω–∞ –∫—É—Ö–Ω—é?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o3 = int(input())
                    if o3 == 1:
                        b2 = random.randint(1, 10)
                        if b2 <= 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —É —Ç–µ–±—è –æ–∫–∞–∑–∞–ª—Å—è –∏–º–º—É–Ω–∏—Ç–µ—Ç –∫ —ç—Ç–æ–π –æ—Ç—Ä–∞–≤–µ.')
                            print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å –Ω–∏—á–µ–≥–æ —Å—ä–µ—Å—Ç–Ω–æ–≥–æ.')
                            print('–ù–æ —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —Ç–∞—Ä–∞–∫–∞–Ω–∏–π –≥–µ–Ω–æ—Ü–∏–¥. –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –≥–µ—Ä–æ–µ–º!')
                            print('–î–µ—Ä–∂–∏ –º–µ–¥–∞–ª—å –∑–∞ —Ö—Ä–∞–±—Ä–æ—Å—Ç—å.')
                            print('ü•á')
                            food -= 10
                            sleep += 10
                        if b2 > 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢—ã –Ω–∞—á–∞–ª –∑–∞–¥—ã—Ö–∞—Ç—å—Å—è.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o3 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å –∏ –Ω–µ –ø–æ—à–µ–ª –≤ –∑–∞—Ä–∞–∂–µ–Ω–Ω—É—é –∑–æ–Ω—É.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            elif otv1 == 2:
                b33 = random.randint(1, 2)
                if b33 == 1:
                    print('–¢—ã –≤—ã—à–µ–ª –∏–∑ –∫–æ–º–Ω–∞—Ç—ã –∏ –ø–æ–ø–∞–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä.')
                    print('–í–¥—Ä—É–≥, –Ω–∞—á–∞–ª—Å—è –Ω–∞–ø–ª—ã–≤ –ª—é–¥–µ–π.')
                    print('–û–≥—Ä–æ–º–Ω–∞—è —Ç–æ–ø–ª–∞ –Ω–∞–¥–≤–∏–≥–∞–ª–∞—Å—å –Ω–∞ —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ü–æ–ø—ã—Ç–∞—Ç—å—Å—è —É–±–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ')
                    o4 = int(input())
                    if o4 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –ø—Ä–∏–¥–µ—Ç—Å—è –ø–æ—Å—Ç–∞—Ä–∞—Ç—å—Å—è –∏ –ø–æ–Ω–∞–¥–µ—è—Ç—å—Å—è –Ω–∞ —Å–≤–æ—é —É–¥–∞—á—É.')
                        print('–¢–µ–±–µ –Ω—É–∂–Ω–æ –≤—ã–±—Ä–∞—Ç—å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤ –∫–æ—Ç–æ—Ä–æ–º –±–µ–∂–∞—Ç—å, —É–≥–∞–¥–∞–ª - –º–æ–ª–æ–¥–µ—Ü, –Ω–µ —É–≥–∞–¥–∞–ª - –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                        print('–ü–æ–µ—Ö–∞–ª–∏!')
                        print('========')
                        nn = 0
                        while nn <= 5:
                            b3=random.randint(1,2)
                            print('–ö—É–¥–∞?')
                            print('1. –í–ª–µ–≤–æ')
                            print('2. –í–ø—Ä–∞–≤–æ')
                            n = int(input())
                            if b3 == 1 and n == 1:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 1 and n == 2:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            elif b3 == 2 and n == 2:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 2 and n == 1:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            else:
                                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                            nn += 1
                            food -= 2
                            sleep += 2
                        print('–£—Ä–∞! –¢—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —ç—Ç–æ–π –±–µ—Å–ø–æ—â–∞–¥–Ω–æ–π –ª—é–¥—Å–∫–æ–π —Ç–æ–ª–ø–µ.')
                    elif o4 == 2:
                        print('–¢—ã –æ—Å—Ç–∞–ª—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ –∏ –ø—Ä–æ—Å—Ç–æ —Å–º–æ—Ç—Ä–µ–ª –∫–∞–∫ —Ç–æ–ª–ø–∞ —Å—Ç–∞–Ω–æ–≤–∏–ª–∞—Å—å –≤—Å–µ –±–ª–∏–∂–µ –∏ –±–ª–∏–∂–µ –∫ —Ç–µ–±–µ.')
                        b4 = random.randint(1, 10)
                        if b4 <= 5:
                            print('–ù–∞ —É–¥–∏–≤–ª–µ–Ω–∏–µ, –Ω–∞ —Ç–µ–±—è –Ω–∏–∫—Ç–æ –¥–∞–∂–µ –Ω–µ –Ω–∞—Å—Ç—É–ø–∏–ª.')
                            print('–¢—ã –æ—á–µ–Ω—å –≤–µ–∑—É—á–∏–π —Ç–∞—Ä–∞–∫–∞–Ω!')
                            sleep += 5
                            food -= 5
                        if b4 > 5:
                            print('–ü–æ —Ç–µ–±–µ –ø—Ä–æ—à–ª–æ—Å—å —Ü–µ–ª—ã—Ö —Ç—Ä–∏ —á–µ–ª–æ–≤–µ–∫–∞.')
                            print('–¢–≤–æ–µ –º–∞–ª–µ–Ω—å–∫–æ–µ —Ç–∞—Ä–∞–∫–∞–Ω—å–µ —Ç–µ–ª—å—Ü–µ –Ω–µ —Å–º–æ–≥–ª–æ –≤—ã–¥–µ—Ä–∂–∞—Ç—å —Ç–∞–∫–æ–≥–æ –Ω–∞–ø–æ—Ä–∞.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                if b33 == 2:
                    print('–¢—ã –≤—ã—à–µ–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä. –°–∑–∞–¥–∏ –ø–æ—Å–ª—ã—à–∞–ª—Å—è –∫–∞–∫–æ–π-—Ç–æ —Ç–æ–ø–æ—Ç.')
                    print('–û–±–µ—Ä–Ω—É–≤—à–∏—Å—å, —Ç—ã —É–≤–∏–¥–µ–ª –≤—ã—Å–æ–∫–æ–≥–æ –º–∞–ª—å—á–∏–∫–∞ –∫–æ—Ç–æ—Ä—ã–π –±–µ–∂–∞–ª –Ω–∞ —Ç–µ–±—è.')
                    print('–≠—Ç–æ –æ–∫–∞–∑–∞–ª—Å—è –°–∞—à–∞ –∏–∑ 205 –∫–æ–º–Ω–∞—Ç—ã. –ï–≥–æ –ª–∏—Ü–æ –≤—ã—Ä–∞–∂–∞–ª–æ —Å–∏–ª—å–Ω–æ–µ –∂–µ–ª–∞–Ω–∏–µ —Ä–∞—Å—Ç–æ–ø—Ç–∞—Ç—å —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ë–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è –Ω–∞ –º–µ—Å—Ç–µ')
                    o5 = int(input())
                    if o5 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –≤–æ–∑–≤–æ–¥–∏ —á–∏—Å–ª–∞ –ø–æ–∫–∞–∑–∞–Ω–Ω—ã–µ –Ω–∞ —ç–∫—Ä–∞–Ω–µ –≤ –∫–≤–∞–¥—Ä–∞—Ç.')
                        igra = 0
                        while igra <= 3:
                            chislo = random.randint(1, 10)
                            print(chislo)
                            kvadrat = int(input())
                            if kvadrat == chislo ** 2:
                                print('–ü—Ä–∞–≤–∏–ª—å–Ω–æ. –ü—Ä–æ–¥–æ–ª–∂–∞–π –±–µ–∂–∞—Ç—å.')
                            else:
                                print('–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ. –°–∞—à–∞ –ø–æ—á—Ç–∏ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è, –Ω–∞—Å—Ç—É–ø–∏–≤ —Ç–µ–±–µ –Ω–∞ –ª–∞–ø–∫—É. –ë—É–¥—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ.')
                                print('–ü–æ–ª—É—á–µ–Ω —É—Ä–æ–Ω.')
                                health -= 10
                            igra += 1
                        chislo = random.randint(1, 10)
                        print(chislo)
                        kvadrat = int(input())
                        if kvadrat == chislo ** 2:
                            print('–£—Ä–∞, —Ç—ã —Å–º–æ–≥ —É–±–µ–∂–∞—Ç—å! –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –Ω–∞—Å—Ç–æ—è—â–∏–º —Å–ø–æ—Ä—Ç—Å–º–µ–Ω–æ–º.')
                        else:
                            print('–°–∞—à–∞ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è –∏ –Ω–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–Ω–æ –Ω–∞—Å—Ç—É–ø–∏–ª –Ω–∞ —Ç–µ–±—è —Å–≤–æ–µ–π –æ–≥—Ä–æ–º–Ω–æ–π –Ω–æ–≥–æ–π.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o5 == 2:
                        print('–°–∞—à–∞ –ø–æ–¥–±–µ–∂–∞–ª –∫ —Ç–µ–±–µ –∏ —Ä–∞–∑–¥–∞–≤–∏–ª.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c += 1
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            print('–¢—ã –≤–µ—Ä–Ω—É–ª—Å—è –≤ –∫–æ–º–Ω–∞—Ç—É.')
        else:
            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    if otvet2==2:
        print('1. –ü–æ–∫—É—à–∞—Ç—å')
        print('2. –ü—Ä–æ–≥—É–ª—è—Ç—å—Å—è')
        otvet4=int(input())
        if otvet4==1:
            food = 50
            sleep += 10
            print('–¢—ã –ø–æ–¥–Ω—è–ª—Å—è –Ω–∞ —Å—Ç–æ–ª –∫ –∑–∞–≤–µ—Ç–Ω—ã–º –∫—Ä–æ—à–∫–∞–º. –ó–∞–≤—Ç—Ä–∞–∫ –≤—ã–¥–∞–ª—Å—è –≤–∫—É—Å–Ω—ã–º –∏ –ø–∏—Ç–∞—Ç–µ–ª—å–Ω—ã–º.')
        elif otvet4==2:
            print('–¢—ã —Ä–µ—à–∏–ª –ø—Ä–æ–≥—É–ª—è—Ç—å—Å—è. –ö—É–¥–∞ –ø–æ–π–¥–µ–º?')
            print('1. –ù–∞ –∫—É—Ö–Ω—é')
            print('2. –í –∫–æ—Ä–∏–¥–æ—Ä')
            otv1 = int(input())
            if otv1 == 1:
                variant = random.randint(1, 3)
                if variant == 1:
                    print('–£ –ø–ª–∏—Ç—ã —Å—Ç–æ—è–ª –º–∞–ª—å—á–∏–∫. –û–Ω —á–µ–º-—Ç–æ —É–≤–ª–µ—á–µ–Ω–Ω–æ –∑–∞–Ω–∏–º–∞–ª—Å—è.')
                    print('–û–∫–∞–∑–∞–ª–æ—Å—å, –æ–Ω –ø–µ–∫ –ø–∏—Ä–æ–≥–∏. –û–¥–Ω–∞ –ø–∞—Ä—Ç–∏—è —É–∂–µ —Å—Ç–æ—è–ª–∞ –Ω–∞ —Å—Ç–æ–ª–µ –∏ –≤—ã–≥–ª—è–¥–µ–ª–∞ –æ—á–µ–Ω—å –∞–ø–ø–µ—Ç–∏—Ç–Ω–æ.')
                    print('–ü–æ–ø—ã—Ç–∞—Ç—å—Å—è –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o1 = int(input())
                    if o1 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –æ—Ç–≤–µ–¥–∞—Ç—å —Å–ª–∞–¥–∫–æ–≥–æ –≤–∫—É—Å–Ω–æ–≥–æ –ø–∏—Ä–æ–≥–∞.')
                        a = random.randint(1, 10)
                        if a <= 5:
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã –æ—Å—Ç–∞–ª—Å—è –Ω–µ–∑–∞–º–µ—á–µ–Ω—ã–º. –¢—ã –ø–ª–æ—Ç–Ω–æ –ø–æ–æ–±–µ–¥–∞–ª –∏ –æ—Å—Ç–∞–ª—Å—è –¥–æ–≤–æ–ª–µ–Ω.')
                            food = 50
                            sleep += 10
                        if a > 5:
                            print('–û –Ω–µ—Ç, —Ç–µ–±—è –∑–∞–º–µ—Ç–∏–ª–∏ –∏ –ø–æ–ø—ã—Ç–∞–ª–∏—Å—å –ø—Ä–∏—Ö–ª–æ–ø–Ω—É—Ç—å.')
                            a1 = random.randint(1, 10)
                            if a1 <= 7:
                                print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å, –Ω–æ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω. –°—Ç–æ–∏—Ç –±—ã—Ç—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ!')
                                print('–£–≤—ã, —Ç—ã –Ω–µ —É—Å–ø–µ–ª —É—Ö–≤–∞—Ç–∏—Ç—å –¥–∞–∂–µ –∫—Ä–æ—à–∫—É.')
                                food -= 10
                                sleep += 15
                                health -= 20
                            if a1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª')
                                c += 1
                    elif o1 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –ø–µ—Ä–µ–±–æ—Ä–æ–ª —Å–≤–æ—é —Ç—è–≥—É –∫–æ –≤–∫—É—Å–Ω—ã–º –ø–∏—Ä–æ–∂–∫–∞–º.')
                        print('–î–æ–≥–æ—Ç–æ–≤–∏–≤, –º–∞–ª—å—á–∏–∫ —É—à–µ–ª. –£–≤—ã, –∑–∞ —Å–æ–±–æ–π –æ–Ω –Ω–µ –æ—Å—Ç–∞–≤–∏–ª –Ω–∏ –æ–¥–Ω–æ–π –∫—Ä–æ—à–∫–∏. –¢–µ–±–µ –Ω–µ –ø–æ–≤–µ–∑–ª–æ.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 2:
                    print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–∏–∫–æ–≥–æ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å. –í —Ä–∞–∫–æ–≤–∏–Ω–µ —Ç—ã —É–≤–∏–¥–µ–ª –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ –∫–∞–ø–ª–∏ –≤–æ–¥—ã.')
                    print('–•–æ—á–µ—à—å –ø–æ–ø–∏—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o2 = int(input())
                    if o2 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –∏—Å–ø–∏—Ç—å –≤–æ–¥—ã, –∑–∞–ª–µ–∑ –≤ —Ä–∞–∫–æ–≤–∏–Ω—É –∏ –ø—Ä–∏–Ω—è–ª—Å—è –∑–∞ –¥–µ–ª–æ.')
                        b = random.randint(1, 10)
                        if b <= 5:
                            print('–¢—ã –Ω–∞–ø–∏–ª—Å—è –≤–¥–æ–≤–æ–ª—å. –¢–≤–æ–µ–º—É —Å—á–∞—Å—Ç—å—é –Ω–µ—Ç –ø—Ä–µ–¥–µ–ª–∞.')
                            food = 50
                            sleep += 10
                        if b > 5:
                            print('–ö–∞–∫ —Ç–æ–ª—å–∫–æ —Ç—ã –Ω–∞—á–∞–ª –ø–∏—Ç—å —Å—Ç–æ–ª—å –∂–µ–ª–∞–Ω–Ω—É—é –≤–æ–¥—É, –∫—Ç–æ-—Ç–æ –∑–∞—à–µ–ª –Ω–∞ –∫—É—Ö–Ω—é.')
                            print('–ù–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–ª—å–Ω–æ –≤–∫–ª—é—á–∏–ª—Å—è –∫—Ä–∞–Ω –∏ —Ç–µ–±—è –Ω–∞—á–∞–ª–æ —Å–Ω–æ—Å–∏—Ç—å –ø–æ—Ç–æ–∫–æ–º –≤–æ–¥—ã.')
                            b1 = random.randint(1, 10)
                            if b1 <= 7:
                                print('–ö —Å—á–∞—Å—Ç—å—é, —Ç–µ–±–µ —É–¥–∞–ª–æ—Å—å –≤—ã–∂–∏—Ç—å. –¢—ã –µ–ª–µ –≤—ã–±—Ä–∞–ª—Å—è –∏ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                                print('–¢—ã –æ—Ç–±–µ–∂–∞–ª –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ, –≥–¥–µ —Ç–µ–±—è —Ç–æ—á–Ω–æ –Ω–µ –Ω–∞–π–¥—É—Ç.')
                                food = 50
                                sleep += 15
                                health -= 20
                            if b1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                                c += 1
                    elif o2 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –æ—Å—Ç–∞–ª—Å—è –Ω–∞–±–ª—é–¥–∞—Ç—å –∑–∞ –∫—É—Ö–Ω–µ–π –≤ —É–≥–æ–ª–∫–µ.')
                        print('–í—Ä–µ–º—è –æ—Ç –≤—Ä–µ–º–µ–Ω–∏ –Ω–∞ –∫—É—Ö–Ω—é –∑–∞—Ö–æ–¥–∏–ª–∏ –ª—é–¥–∏ –∏ —á—Ç–æ-—Ç–æ –¥–µ–ª–∞–ª–∏.')
                        print('–¢—ã –¥—É–º–∞–ª –æ —Ç–æ–º, –∫–∞–∫–æ–≤–æ –±—ã—Ç—å —á–µ–ª–æ–≤–µ–∫–æ–º.')
                        print('–õ—é–¥–∏ —Ç–∞–∫–∏–µ –±–æ–ª—å—à–∏–µ, –ø–æ—Å—Ç–æ—è–Ω–Ω–æ —á–µ–º-—Ç–æ –∑–∞–Ω—è—Ç—ã, –∫—É–¥–∞-—Ç–æ —Ö–æ–¥—è—Ç, —Ä–∞–¥—É—é—Ç—Å—è, –≥—Ä—É—Å—Ç—è—Ç.')
                        print('–ö–∞–∫–æ–≤–∞ –∏—Ö —Ü–µ–ª—å? –û —á–µ–º –æ–Ω–∏ –¥—É–º–∞—é—Ç? –í—Å–µ —ç—Ç–∏ –≤–æ–ø—Ä–æ—Å—ã –æ—á–µ–Ω—å –∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–ª–∏ —Ç–µ–±—è.')
                        print('–ù–æ –æ—Ç–≤–µ—Ç—ã –Ω–∞ –Ω–∏—Ö —Ç—ã –≤—Ä—è–¥ –ª–∏ –ø–æ–ª—É—á–∏—à—å.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 3:
                    print('–û –Ω–µ—Ç, –Ω–∞ –∫—É—Ö–Ω–µ —Ç—Ä–∞–≤—è—Ç —Ç–∞—Ä–∞–∫–∞–Ω–æ–≤. –ï—Å–ª–∏ —Ö–æ—á–µ—à—å –≤—ã–∂–∏—Ç—å, —Ç–æ —Ç–µ–±–µ –ª—É—á—à–µ —Ç—É–¥–∞ –Ω–µ —Ö–æ–¥–∏—Ç—å.')
                    print('–í—Å–µ —Ä–∞–≤–Ω–æ –ø–æ–π—Ç–∏ –Ω–∞ –∫—É—Ö–Ω—é?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o3 = int(input())
                    if o3 == 1:
                        b2 = random.randint(1, 10)
                        if b2 <= 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —É —Ç–µ–±—è –æ–∫–∞–∑–∞–ª—Å—è –∏–º–º—É–Ω–∏—Ç–µ—Ç –∫ —ç—Ç–æ–π –æ—Ç—Ä–∞–≤–µ.')
                            print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å –Ω–∏—á–µ–≥–æ —Å—ä–µ—Å—Ç–Ω–æ–≥–æ.')
                            print('–ù–æ —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —Ç–∞—Ä–∞–∫–∞–Ω–∏–π –≥–µ–Ω–æ—Ü–∏–¥. –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –≥–µ—Ä–æ–µ–º!')
                            print('–î–µ—Ä–∂–∏ –º–µ–¥–∞–ª—å –∑–∞ —Ö—Ä–∞–±—Ä–æ—Å—Ç—å.')
                            print('ü•á')
                            food -= 10
                            sleep += 10
                        if b2 > 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢—ã –Ω–∞—á–∞–ª –∑–∞–¥—ã—Ö–∞—Ç—å—Å—è.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o3 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å –∏ –Ω–µ –ø–æ—à–µ–ª –≤ –∑–∞—Ä–∞–∂–µ–Ω–Ω—É—é –∑–æ–Ω—É.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            elif otv1 == 2:
                b33 = random.randint(1, 2)
                if b33 == 1:
                    print('–¢—ã –≤—ã—à–µ–ª –∏–∑ –∫–æ–º–Ω–∞—Ç—ã –∏ –ø–æ–ø–∞–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä.')
                    print('–í–¥—Ä—É–≥, –Ω–∞—á–∞–ª—Å—è –Ω–∞–ø–ª—ã–≤ –ª—é–¥–µ–π.')
                    print('–û–≥—Ä–æ–º–Ω–∞—è —Ç–æ–ø–ª–∞ –Ω–∞–¥–≤–∏–≥–∞–ª–∞—Å—å –Ω–∞ —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ü–æ–ø—ã—Ç–∞—Ç—å—Å—è —É–±–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ')
                    o4 = int(input())
                    if o4 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –ø—Ä–∏–¥–µ—Ç—Å—è –ø–æ—Å—Ç–∞—Ä–∞—Ç—å—Å—è –∏ –ø–æ–Ω–∞–¥–µ—è—Ç—å—Å—è –Ω–∞ —Å–≤–æ—é —É–¥–∞—á—É.')
                        print('–¢–µ–±–µ –Ω—É–∂–Ω–æ –≤—ã–±—Ä–∞—Ç—å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤ –∫–æ—Ç–æ—Ä–æ–º –±–µ–∂–∞—Ç—å, —É–≥–∞–¥–∞–ª - –º–æ–ª–æ–¥–µ—Ü, –Ω–µ —É–≥–∞–¥–∞–ª - –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                        print('–ü–æ–µ—Ö–∞–ª–∏!')
                        print('========')
                        b3 = random.randint(1, 2)
                        nn = 0
                        while nn <= 5:
                            print('–ö—É–¥–∞?')
                            print('1. –í–ª–µ–≤–æ')
                            print('2. –í–ø—Ä–∞–≤–æ')
                            n = int(input())
                            if b3 == 1 and n == 1:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 1 and n == 2:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            elif b3 == 2 and n == 2:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 2 and n == 1:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            else:
                                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                            nn += 1
                            food -= 2
                            sleep += 2
                        print('–£—Ä–∞! –¢—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —ç—Ç–æ–π –±–µ—Å–ø–æ—â–∞–¥–Ω–æ–π –ª—é–¥—Å–∫–æ–π —Ç–æ–ª–ø–µ.')
                    elif o4 == 2:
                        print('–¢—ã –æ—Å—Ç–∞–ª—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ –∏ –ø—Ä–æ—Å—Ç–æ —Å–º–æ—Ç—Ä–µ–ª –∫–∞–∫ —Ç–æ–ª–ø–∞ —Å—Ç–∞–Ω–æ–≤–∏–ª–∞—Å—å –≤—Å–µ –±–ª–∏–∂–µ –∏ –±–ª–∏–∂–µ –∫ —Ç–µ–±–µ.')
                        b4 = random.randint(1, 10)
                        if b4 <= 5:
                            print('–ù–∞ —É–¥–∏–≤–ª–µ–Ω–∏–µ, –Ω–∞ —Ç–µ–±—è –Ω–∏–∫—Ç–æ –¥–∞–∂–µ –Ω–µ –Ω–∞—Å—Ç—É–ø–∏–ª.')
                            print('–¢—ã –æ—á–µ–Ω—å –≤–µ–∑—É—á–∏–π —Ç–∞—Ä–∞–∫–∞–Ω!')
                            sleep += 5
                            food -= 5
                        if b4 > 5:
                            print('–ü–æ —Ç–µ–±–µ –ø—Ä–æ—à–ª–æ—Å—å —Ü–µ–ª—ã—Ö —Ç—Ä–∏ —á–µ–ª–æ–≤–µ–∫–∞.')
                            print('–¢–≤–æ–µ –º–∞–ª–µ–Ω—å–∫–æ–µ —Ç–∞—Ä–∞–∫–∞–Ω—å–µ —Ç–µ–ª—å—Ü–µ –Ω–µ —Å–º–æ–≥–ª–æ –≤—ã–¥–µ—Ä–∂–∞—Ç—å —Ç–∞–∫–æ–≥–æ –Ω–∞–ø–æ—Ä–∞.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                if b33 == 2:
                    print('–¢—ã –≤—ã—à–µ–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä. –°–∑–∞–¥–∏ –ø–æ—Å–ª—ã—à–∞–ª—Å—è –∫–∞–∫–æ–π-—Ç–æ —Ç–æ–ø–æ—Ç.')
                    print('–û–±–µ—Ä–Ω—É–≤—à–∏—Å—å, —Ç—ã —É–≤–∏–¥–µ–ª –≤—ã—Å–æ–∫–æ–≥–æ –º–∞–ª—å—á–∏–∫–∞ –∫–æ—Ç–æ—Ä—ã–π –±–µ–∂–∞–ª –Ω–∞ —Ç–µ–±—è.')
                    print('–≠—Ç–æ –æ–∫–∞–∑–∞–ª—Å—è –°–∞—à–∞ –∏–∑ 205 –∫–æ–º–Ω–∞—Ç—ã. –ï–≥–æ –ª–∏—Ü–æ –≤—ã—Ä–∞–∂–∞–ª–æ —Å–∏–ª—å–Ω–æ–µ –∂–µ–ª–∞–Ω–∏–µ —Ä–∞—Å—Ç–æ–ø—Ç–∞—Ç—å —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ë–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è –Ω–∞ –º–µ—Å—Ç–µ')
                    o5 = int(input())
                    if o5 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –≤–æ–∑–≤–æ–¥–∏ —á–∏—Å–ª–∞ –ø–æ–∫–∞–∑–∞–Ω–Ω—ã–µ –Ω–∞ —ç–∫—Ä–∞–Ω–µ –≤ –∫–≤–∞–¥—Ä–∞—Ç.')
                        igra = 0
                        while igra <= 3:
                            chislo = random.randint(1, 10)
                            print(chislo)
                            kvadrat = int(input())
                            if kvadrat == chislo ** 2:
                                print('–ü—Ä–∞–≤–∏–ª—å–Ω–æ. –ü—Ä–æ–¥–æ–ª–∂–∞–π –±–µ–∂–∞—Ç—å.')
                            else:
                                print('–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ. –°–∞—à–∞ –ø–æ—á—Ç–∏ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è, –Ω–∞—Å—Ç—É–ø–∏–≤ —Ç–µ–±–µ –Ω–∞ –ª–∞–ø–∫—É. –ë—É–¥—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ.')
                                print('–ü–æ–ª—É—á–µ–Ω —É—Ä–æ–Ω.')
                                health -= 10
                            igra += 1
                        chislo = random.randint(1, 10)
                        print(chislo)
                        kvadrat = int(input())
                        if kvadrat == chislo ** 2:
                            print('–£—Ä–∞, —Ç—ã —Å–º–æ–≥ —É–±–µ–∂–∞—Ç—å! –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –Ω–∞—Å—Ç–æ—è—â–∏–º —Å–ø–æ—Ä—Ç—Å–º–µ–Ω–æ–º.')
                        else:
                            print('–°–∞—à–∞ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è –∏ –Ω–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–Ω–æ –Ω–∞—Å—Ç—É–ø–∏–ª –Ω–∞ —Ç–µ–±—è —Å–≤–æ–µ–π –æ–≥—Ä–æ–º–Ω–æ–π –Ω–æ–≥–æ–π.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o5 == 2:
                        print('–°–∞—à–∞ –ø–æ–¥–±–µ–∂–∞–ª –∫ —Ç–µ–±–µ –∏ —Ä–∞–∑–¥–∞–≤–∏–ª.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c += 1
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            print('–¢—ã –≤–µ—Ä–Ω—É–ª—Å—è –≤ –∫–æ–º–Ω–∞—Ç—É.')
        else:
            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    if otvet2==3:
        print('1. –ü–æ–∫—É—à–∞—Ç—å')
        print('2. –ü–æ—Å–ø–∞—Ç—å')
        otvet5=int(input())
        if otvet5==1:
            print('–ù–∞ —Å—Ç–æ–ª–µ –ª–µ–∂–∞–ª–∏ –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ —Ö–ª–µ–±–Ω—ã–µ –∫—Ä–æ—à–∫–∏. –¢—ã –∑–∞–ª–µ–∑ –Ω–∞ –Ω–µ–≥–æ –∏ –≤–∫—É—Å–Ω–æ –ø–æ–∫—É—à–∞–ª.')
            food = 50
            sleep += 10
        elif otvet5==2:
            print('–¢—ã —Ä–µ—à–∏–ª –ª–µ—á—å –ø–æ—Å–ø–∞—Ç—å. –ü—Ä–æ—Å–ø–∞–≤ –¥–æ –≤–µ—á–µ—Ä–∞, —Ç—ã –ø—Ä–æ—Å–Ω—É–ª—Å—è.')
            print('–ñ–∏—Ç–µ–ª—å–Ω–∏—Ü—ã –∫–æ–º–Ω–∞—Ç—ã —É–∂–µ –≤–µ—Ä–Ω—É–ª–∏—Å—å. –ù–∞—Ö–æ–¥–∏—Ç—å—Å—è –≤ –∫–æ–º–Ω–∞—Ç–µ —Å—Ç–∞–ª–æ –Ω–µ–º–Ω–æ–≥–æ –æ–ø–∞—Å–Ω–æ.')
            food -= 10
            sleep = 0
        else:
            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    print(f'–ó–¥–æ—Ä–æ–≤—å–µ: {health}')
    print(f'–°–æ–Ω–ª–∏–≤–æ—Å—Ç—å: {sleep}')
    print(f'–°—ã—Ç–æ—Å—Ç—å: {food}')
    print('–û—Ç–ª–∏—á–Ω–æ. –ß–µ–º –∑–∞–π–º–µ—à—å—Å—è –¥–∞–ª—å—à–µ?')
    if otvet3==1 or otvet4==1:
        print('1. –ü—Ä–æ–≥—É–ª—è—Ç—å—Å—è')
        otvet6=int(input())
        if otvet6==1:
            print('–¢—ã —Ä–µ—à–∏–ª –ø—Ä–æ–≥—É–ª—è—Ç—å—Å—è. –ö—É–¥–∞ –ø–æ–π–¥–µ–º?')
            print('1. –ù–∞ –∫—É—Ö–Ω—é')
            print('2. –í –∫–æ—Ä–∏–¥–æ—Ä')
            otv1 = int(input())
            if otv1 == 1:
                variant = random.randint(1, 3)
                if variant == 1:
                    print('–£ –ø–ª–∏—Ç—ã —Å—Ç–æ—è–ª –º–∞–ª—å—á–∏–∫. –û–Ω —á–µ–º-—Ç–æ —É–≤–ª–µ—á–µ–Ω–Ω–æ –∑–∞–Ω–∏–º–∞–ª—Å—è.')
                    print('–û–∫–∞–∑–∞–ª–æ—Å—å, –æ–Ω –ø–µ–∫ –ø–∏—Ä–æ–≥–∏. –û–¥–Ω–∞ –ø–∞—Ä—Ç–∏—è —É–∂–µ —Å—Ç–æ—è–ª–∞ –Ω–∞ —Å—Ç–æ–ª–µ –∏ –≤—ã–≥–ª—è–¥–µ–ª–∞ –æ—á–µ–Ω—å –∞–ø–ø–µ—Ç–∏—Ç–Ω–æ.')
                    print('–ü–æ–ø—ã—Ç–∞—Ç—å—Å—è –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o1 = int(input())
                    if o1 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –æ—Ç–≤–µ–¥–∞—Ç—å —Å–ª–∞–¥–∫–æ–≥–æ –≤–∫—É—Å–Ω–æ–≥–æ –ø–∏—Ä–æ–≥–∞.')
                        a = random.randint(1, 10)
                        if a <= 5:
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã –æ—Å—Ç–∞–ª—Å—è –Ω–µ–∑–∞–º–µ—á–µ–Ω—ã–º. –¢—ã –ø–ª–æ—Ç–Ω–æ –ø–æ–æ–±–µ–¥–∞–ª –∏ –æ—Å—Ç–∞–ª—Å—è –¥–æ–≤–æ–ª–µ–Ω.')
                            food = 50
                            sleep += 10
                        if a > 5:
                            print('–û –Ω–µ—Ç, —Ç–µ–±—è –∑–∞–º–µ—Ç–∏–ª–∏ –∏ –ø–æ–ø—ã—Ç–∞–ª–∏—Å—å –ø—Ä–∏—Ö–ª–æ–ø–Ω—É—Ç—å.')
                            a1 = random.randint(1, 10)
                            if a1 <= 7:
                                print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å, –Ω–æ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω. –°—Ç–æ–∏—Ç –±—ã—Ç—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ!')
                                print('–£–≤—ã, —Ç—ã –Ω–µ —É—Å–ø–µ–ª —É—Ö–≤–∞—Ç–∏—Ç—å –¥–∞–∂–µ –∫—Ä–æ—à–∫—É.')
                                food -= 10
                                sleep += 15
                                health -= 20
                            if a1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                                c += 1
                    elif o1 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –ø–µ—Ä–µ–±–æ—Ä–æ–ª —Å–≤–æ—é —Ç—è–≥—É –∫–æ –≤–∫—É—Å–Ω—ã–º –ø–∏—Ä–æ–∂–∫–∞–º.')
                        print('–î–æ–≥–æ—Ç–æ–≤–∏–≤, –º–∞–ª—å—á–∏–∫ —É—à–µ–ª. –£–≤—ã, –∑–∞ —Å–æ–±–æ–π –æ–Ω –Ω–µ –æ—Å—Ç–∞–≤–∏–ª –Ω–∏ –æ–¥–Ω–æ–π –∫—Ä–æ—à–∫–∏. –¢–µ–±–µ –Ω–µ –ø–æ–≤–µ–∑–ª–æ.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 2:
                    print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–∏–∫–æ–≥–æ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å. –í —Ä–∞–∫–æ–≤–∏–Ω–µ —Ç—ã —É–≤–∏–¥–µ–ª –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ –∫–∞–ø–ª–∏ –≤–æ–¥—ã.')
                    print('–•–æ—á–µ—à—å –ø–æ–ø–∏—Ç—å?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o2 = int(input())
                    if o2 == 1:
                        print('–¢—ã —Ä–µ—à–∏–ª –∏—Å–ø–∏—Ç—å –≤–æ–¥—ã, –∑–∞–ª–µ–∑ –≤ —Ä–∞–∫–æ–≤–∏–Ω—É –∏ –ø—Ä–∏–Ω—è–ª—Å—è –∑–∞ –¥–µ–ª–æ.')
                        b = random.randint(1, 10)
                        if b <= 5:
                            print('–¢—ã –Ω–∞–ø–∏–ª—Å—è –≤–¥–æ–≤–æ–ª—å. –¢–≤–æ–µ–º—É —Å—á–∞—Å—Ç—å—é –Ω–µ—Ç –ø—Ä–µ–¥–µ–ª–∞.')
                            food = 50
                            sleep += 10
                        if b > 5:
                            print('–ö–∞–∫ —Ç–æ–ª—å–∫–æ —Ç—ã –Ω–∞—á–∞–ª –ø–∏—Ç—å —Å—Ç–æ–ª—å –∂–µ–ª–∞–Ω–Ω—É—é –≤–æ–¥—É, –∫—Ç–æ-—Ç–æ –∑–∞—à–µ–ª –Ω–∞ –∫—É—Ö–Ω—é.')
                            print('–ù–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–ª—å–Ω–æ –≤–∫–ª—é—á–∏–ª—Å—è –∫—Ä–∞–Ω –∏ —Ç–µ–±—è –Ω–∞—á–∞–ª–æ —Å–Ω–æ—Å–∏—Ç—å –ø–æ—Ç–æ–∫–æ–º –≤–æ–¥—ã.')
                            b1 = random.randint(1, 10)
                            if b1 <= 7:
                                print('–ö —Å—á–∞—Å—Ç—å—é, —Ç–µ–±–µ —É–¥–∞–ª–æ—Å—å –≤—ã–∂–∏—Ç—å. –¢—ã –µ–ª–µ –≤—ã–±—Ä–∞–ª—Å—è –∏ –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                                print('–¢—ã –æ—Ç–±–µ–∂–∞–ª –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ, –≥–¥–µ —Ç–µ–±—è —Ç–æ—á–Ω–æ –Ω–µ –Ω–∞–π–¥—É—Ç.')
                                food = 50
                                sleep += 15
                                health -= 20
                            if b1 > 7:
                                print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                                print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                                c += 1
                    elif o2 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–µ–π –∂–∏–∑–Ω—å—é –∏ –æ—Å—Ç–∞–ª—Å—è –Ω–∞–±–ª—é–¥–∞—Ç—å –∑–∞ –∫—É—Ö–Ω–µ–π –≤ —É–≥–æ–ª–∫–µ.')
                        print('–í—Ä–µ–º—è –æ—Ç –≤—Ä–µ–º–µ–Ω–∏ –Ω–∞ –∫—É—Ö–Ω—é –∑–∞—Ö–æ–¥–∏–ª–∏ –ª—é–¥–∏ –∏ —á—Ç–æ-—Ç–æ –¥–µ–ª–∞–ª–∏.')
                        print('–¢—ã –¥—É–º–∞–ª –æ —Ç–æ–º, –∫–∞–∫–æ–≤–æ –±—ã—Ç—å —á–µ–ª–æ–≤–µ–∫–æ–º.')
                        print('–õ—é–¥–∏ —Ç–∞–∫–∏–µ –±–æ–ª—å—à–∏–µ, –ø–æ—Å—Ç–æ—è–Ω–Ω–æ —á–µ–º-—Ç–æ –∑–∞–Ω—è—Ç—ã, –∫—É–¥–∞-—Ç–æ —Ö–æ–¥—è—Ç, —Ä–∞–¥—É—é—Ç—Å—è, –≥—Ä—É—Å—Ç—è—Ç.')
                        print('–ö–∞–∫–æ–≤–∞ –∏—Ö —Ü–µ–ª—å? –û —á–µ–º –æ–Ω–∏ –¥—É–º–∞—é—Ç? –í—Å–µ —ç—Ç–∏ –≤–æ–ø—Ä–æ—Å—ã –æ—á–µ–Ω—å –∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–ª–∏ —Ç–µ–±—è.')
                        print('–ù–æ –æ—Ç–≤–µ—Ç—ã –Ω–∞ –Ω–∏—Ö —Ç—ã –≤—Ä—è–¥ –ª–∏ –ø–æ–ª—É—á–∏—à—å.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                elif variant == 3:
                    print('–û –Ω–µ—Ç, –Ω–∞ –∫—É—Ö–Ω–µ —Ç—Ä–∞–≤—è—Ç —Ç–∞—Ä–∞–∫–∞–Ω–æ–≤. –ï—Å–ª–∏ —Ö–æ—á–µ—à—å –≤—ã–∂–∏—Ç—å, —Ç–æ —Ç–µ–±–µ –ª—É—á—à–µ —Ç—É–¥–∞ –Ω–µ —Ö–æ–¥–∏—Ç—å.')
                    print('–í—Å–µ —Ä–∞–≤–Ω–æ –ø–æ–π—Ç–∏ –Ω–∞ –∫—É—Ö–Ω—é?')
                    print('1. –î–∞')
                    print('2. –ù–µ—Ç')
                    o3 = int(input())
                    if o3 == 1:
                        b2 = random.randint(1, 10)
                        if b2 <= 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢–µ–±–µ –ø–æ–≤–µ–∑–ª–æ, —É —Ç–µ–±—è –æ–∫–∞–∑–∞–ª—Å—è –∏–º–º—É–Ω–∏—Ç–µ—Ç –∫ —ç—Ç–æ–π –æ—Ç—Ä–∞–≤–µ.')
                            print('–ù–∞ –∫—É—Ö–Ω–µ –Ω–µ –æ–∫–∞–∑–∞–ª–æ—Å—å –Ω–∏—á–µ–≥–æ —Å—ä–µ—Å—Ç–Ω–æ–≥–æ.')
                            print('–ù–æ —Ç—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —Ç–∞—Ä–∞–∫–∞–Ω–∏–π –≥–µ–Ω–æ—Ü–∏–¥. –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –≥–µ—Ä–æ–µ–º!')
                            print('–î–µ—Ä–∂–∏ –º–µ–¥–∞–ª—å –∑–∞ —Ö—Ä–∞–±—Ä–æ—Å—Ç—å.')
                            print('ü•á')
                            food -= 10
                            sleep += 10
                        if b2 > 4:
                            print('–ê —Ç—ã –±–µ—Å—Å—Ç—Ä–∞—à–Ω—ã–π —Ç–∞—Ä–∞–∫–∞–Ω. –í–æ–π–¥—è –Ω–∞ –∫—É—Ö–Ω—é, —Ç–µ–±–µ –≤ –¥—ã—Ö–∞–ª—å—Ü–∞ —É–¥–∞—Ä–∏–ª –Ω–µ–ø—Ä–∏—è—Ç–Ω—ã–π –∑–∞–ø–∞—Ö.')
                            print('–¢—ã –Ω–∞—á–∞–ª –∑–∞–¥—ã—Ö–∞—Ç—å—Å—è.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o3 == 2:
                        print('–¢—ã —Ä–µ—à–∏–ª –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å –∏ –Ω–µ –ø–æ—à–µ–ª –≤ –∑–∞—Ä–∞–∂–µ–Ω–Ω—É—é –∑–æ–Ω—É.')
                        food -= 10
                        sleep += 10
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            elif otv1 == 2:
                b33 = random.randint(1, 2)
                if b33 == 1:
                    print('–¢—ã –≤—ã—à–µ–ª –∏–∑ –∫–æ–º–Ω–∞—Ç—ã –∏ –ø–æ–ø–∞–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä.')
                    print('–í–¥—Ä—É–≥, –Ω–∞—á–∞–ª—Å—è –Ω–∞–ø–ª—ã–≤ –ª—é–¥–µ–π.')
                    print('–û–≥—Ä–æ–º–Ω–∞—è —Ç–æ–ø–ª–∞ –Ω–∞–¥–≤–∏–≥–∞–ª–∞—Å—å –Ω–∞ —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ü–æ–ø—ã—Ç–∞—Ç—å—Å—è —É–±–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ')
                    o4 = int(input())
                    if o4 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –ø—Ä–∏–¥–µ—Ç—Å—è –ø–æ—Å—Ç–∞—Ä–∞—Ç—å—Å—è –∏ –ø–æ–Ω–∞–¥–µ—è—Ç—å—Å—è –Ω–∞ —Å–≤–æ—é —É–¥–∞—á—É.')
                        print('–¢–µ–±–µ –Ω—É–∂–Ω–æ –≤—ã–±—Ä–∞—Ç—å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤ –∫–æ—Ç–æ—Ä–æ–º –±–µ–∂–∞—Ç—å, —É–≥–∞–¥–∞–ª - –º–æ–ª–æ–¥–µ—Ü, –Ω–µ —É–≥–∞–¥–∞–ª - –ø–æ–ª—É—á–∏–ª —É—Ä–æ–Ω.')
                        print('–ü–æ–µ—Ö–∞–ª–∏!')
                        print('========')
                        nn = 0
                        while nn <= 5:
                            b3=random.randint(1,2)
                            print('–ö—É–¥–∞?')
                            print('1. –í–ª–µ–≤–æ')
                            print('2. –í–ø—Ä–∞–≤–æ')
                            n = int(input())
                            if b3 == 1 and n == 1:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 1 and n == 2:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            elif b3 == 2 and n == 2:
                                print('–ú–æ–ª–æ–¥–µ—Ü, —Ç—ã —Å–º–æ–≥ —É–∫–ª–æ–Ω–∏—Ç—å—Å—è!')
                            elif b3 == 2 and n == 1:
                                print('–û –Ω–µ—Ç, –Ω–∞ —Ç–µ–±—è –Ω–∞—Å—Ç—É–ø–∏–ª–∏...')
                                health -= 5
                            else:
                                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                            nn += 1
                            food -= 2
                            sleep += 2
                        print('–£—Ä–∞! –¢—ã —Å–º–æ–≥ –≤—ã–∂–∏—Ç—å –≤ —ç—Ç–æ–π –±–µ—Å–ø–æ—â–∞–¥–Ω–æ–π –ª—é–¥—Å–∫–æ–π —Ç–æ–ª–ø–µ.')
                    elif o4 == 2:
                        print('–¢—ã –æ—Å—Ç–∞–ª—Å—è —Å—Ç–æ—è—Ç—å –Ω–∞ –º–µ—Å—Ç–µ –∏ –ø—Ä–æ—Å—Ç–æ —Å–º–æ—Ç—Ä–µ–ª –∫–∞–∫ —Ç–æ–ª–ø–∞ —Å—Ç–∞–Ω–æ–≤–∏–ª–∞—Å—å –≤—Å–µ –±–ª–∏–∂–µ –∏ –±–ª–∏–∂–µ –∫ —Ç–µ–±–µ.')
                        b4 = random.randint(1, 10)
                        if b4 <= 5:
                            print('–ù–∞ —É–¥–∏–≤–ª–µ–Ω–∏–µ, –Ω–∞ —Ç–µ–±—è –Ω–∏–∫—Ç–æ –¥–∞–∂–µ –Ω–µ –Ω–∞—Å—Ç—É–ø–∏–ª.')
                            print('–¢—ã –æ—á–µ–Ω—å –≤–µ–∑—É—á–∏–π —Ç–∞—Ä–∞–∫–∞–Ω!')
                            sleep += 5
                            food -= 5
                        if b4 > 5:
                            print('–ü–æ —Ç–µ–±–µ –ø—Ä–æ—à–ª–æ—Å—å —Ü–µ–ª—ã—Ö —Ç—Ä–∏ —á–µ–ª–æ–≤–µ–∫–∞.')
                            print('–¢–≤–æ–µ –º–∞–ª–µ–Ω—å–∫–æ–µ —Ç–∞—Ä–∞–∫–∞–Ω—å–µ —Ç–µ–ª—å—Ü–µ –Ω–µ —Å–º–æ–≥–ª–æ –≤—ã–¥–µ—Ä–∂–∞—Ç—å —Ç–∞–∫–æ–≥–æ –Ω–∞–ø–æ—Ä–∞.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    else:
                        print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
                if b33 == 2:
                    print('–¢—ã –≤—ã—à–µ–ª –≤ –∫–æ—Ä–∏–¥–æ—Ä. –°–∑–∞–¥–∏ –ø–æ—Å–ª—ã—à–∞–ª—Å—è –∫–∞–∫–æ–π-—Ç–æ —Ç–æ–ø–æ—Ç.')
                    print('–û–±–µ—Ä–Ω—É–≤—à–∏—Å—å, —Ç—ã —É–≤–∏–¥–µ–ª –≤—ã—Å–æ–∫–æ–≥–æ –º–∞–ª—å—á–∏–∫–∞ –∫–æ—Ç–æ—Ä—ã–π –±–µ–∂–∞–ª –Ω–∞ —Ç–µ–±—è.')
                    print('–≠—Ç–æ –æ–∫–∞–∑–∞–ª—Å—è –°–∞—à–∞ –∏–∑ 205 –∫–æ–º–Ω–∞—Ç—ã. –ï–≥–æ –ª–∏—Ü–æ –≤—ã—Ä–∞–∂–∞–ª–æ —Å–∏–ª—å–Ω–æ–µ –∂–µ–ª–∞–Ω–∏–µ —Ä–∞—Å—Ç–æ–ø—Ç–∞—Ç—å —Ç–µ–±—è.')
                    print('–ß—Ç–æ –±—É–¥–µ—à—å –¥–µ–ª–∞—Ç—å?')
                    print('1. –ë–µ–∂–∞—Ç—å')
                    print('2. –û—Å—Ç–∞—Ç—å—Å—è –Ω–∞ –º–µ—Å—Ç–µ')
                    o5 = int(input())
                    if o5 == 1:
                        print('–ß—Ç–æ–±—ã —É–±–µ–∂–∞—Ç—å –≤–æ–∑–≤–æ–¥–∏ —á–∏—Å–ª–∞ –ø–æ–∫–∞–∑–∞–Ω–Ω—ã–µ –Ω–∞ —ç–∫—Ä–∞–Ω–µ –≤ –∫–≤–∞–¥—Ä–∞—Ç.')
                        igra = 0
                        while igra <= 3:
                            chislo = random.randint(1, 10)
                            print(chislo)
                            kvadrat = int(input())
                            if kvadrat == chislo ** 2:
                                print('–ü—Ä–∞–≤–∏–ª—å–Ω–æ. –ü—Ä–æ–¥–æ–ª–∂–∞–π –±–µ–∂–∞—Ç—å.')
                            else:
                                print('–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ. –°–∞—à–∞ –ø–æ—á—Ç–∏ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è, –Ω–∞—Å—Ç—É–ø–∏–≤ —Ç–µ–±–µ –Ω–∞ –ª–∞–ø–∫—É. –ë—É–¥—å –æ—Å—Ç–æ—Ä–æ–∂–Ω–µ–µ.')
                                print('–ü–æ–ª—É—á–µ–Ω —É—Ä–æ–Ω.')
                                health -= 10
                            igra += 1
                        chislo = random.randint(1, 10)
                        print(chislo)
                        kvadrat = int(input())
                        if kvadrat == chislo ** 2:
                            print('–£—Ä–∞, —Ç—ã —Å–º–æ–≥ —É–±–µ–∂–∞—Ç—å! –ú–æ–∂–µ—à—å —Å—á–∏—Ç–∞—Ç—å —Å–µ–±—è –Ω–∞—Å—Ç–æ—è—â–∏–º —Å–ø–æ—Ä—Ç—Å–º–µ–Ω–æ–º.')
                        else:
                            print('–°–∞—à–∞ –¥–æ–≥–Ω–∞–ª —Ç–µ–±—è –∏ –Ω–µ–∑–∞–º–µ–¥–ª–∏—Ç–µ–Ω–æ –Ω–∞—Å—Ç—É–ø–∏–ª –Ω–∞ —Ç–µ–±—è —Å–≤–æ–µ–π –æ–≥—Ä–æ–º–Ω–æ–π –Ω–æ–≥–æ–π.')
                            print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                            print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                            c += 1
                    elif o5 == 2:
                        print('–°–∞—à–∞ –ø–æ–¥–±–µ–∂–∞–ª –∫ —Ç–µ–±–µ –∏ —Ä–∞–∑–¥–∞–≤–∏–ª.')
                        print('–ö —Å–æ–∂–∞–ª–µ–Ω–∏—é, —Ç—ã —É–º–µ—Ä...')
                        print('–ò–≥—Ä–∞ –æ–∫–æ–Ω—á–µ–Ω–∞. –¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª.')
                        c += 1
                    else:
                            print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            else:
                    print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
            print('–¢—ã –≤–µ—Ä–Ω—É–ª—Å—è –≤ –∫–æ–º–Ω–∞—Ç—É.')
        else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    elif otvet3==2 or otvet5==1:
        print('1. –ü–æ—Å–ø–∞—Ç—å')
        otvet7=int(input())
        if otvet7==1:
            print('–¢—ã —Ä–µ—à–∏–ª –ª–µ—á—å –ø–æ—Å–ø–∞—Ç—å. –¢–µ–±–µ —Å–Ω–∏–ª–∞—Å—å –ª—é–¥—Å–∫–∞—è –∂–∏–∑–Ω—å.')
            food -= 10
            sleep = 0
        else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    else:
        print('1.–ü–æ–∫—É—à–∞—Ç—å')
        otvet8=int(input())
        if otvet8==1:
            print('–ù–∞ —Å—Ç–æ–ª–µ –ª–µ–∂–∞–ª–∏ –∞–ø–ø–µ—Ç–∏—Ç–Ω—ã–µ —Ö–ª–µ–±–Ω—ã–µ –∫—Ä–æ—à–∫–∏. –¢—ã –∑–∞–ª–µ–∑ –Ω–∞ –Ω–µ–≥–æ –∏ –≤–∫—É—Å–Ω–æ –ø–æ–∫—É—à–∞–ª.')
            food = 50
            sleep += 10
        else:
                print('–í–≤–µ–¥–µ–Ω–æ –Ω–µ–¥–æ–ø—É—Å—Ç–∏–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ. –ü–æ–ø—Ä–æ–±—É–π –µ—â–µ —Ä–∞–∑.')
    print(f'–ó–¥–æ—Ä–æ–≤—å–µ: {health}')
    print(f'–°–æ–Ω–ª–∏–≤–æ—Å—Ç—å: {sleep}')
    print(f'–°—ã—Ç–æ—Å—Ç—å: {food}')
    print('===============')
    print('    –î–µ–Ω—å  2')
    print('===============')
    break

